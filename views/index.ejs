<!DOCTYPE html>
<html lang="fa">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style-prefix.css">
   
   
    <!-- 
      - google font link
    -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">

    <!-- swiper css link  -->
    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css" />
  
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap"
      rel="stylesheet">
    <title>فروشگاه تی تی</title>
</head>
<body>
   
    <%- include('./partials/modal') %> 
    <%- include('./partials/header',{  gh,
        ds,
        gho}); %> 
    <main>

        <!--
          - BANNER
        -->
    
        <div class="banner">
    
          <div class="container">
    
            <div class="slider-container has-scrollbar">
    <% gh.forEach((item)=>{%>
        <div class="slider-item">
    
            <img src="<%=item.image%>" alt="women's latest fashion sale" class="banner-img">

            <div class="banner-content">

               <p class="banner-subtitle">جدید ترین محصولات</p>

              <h2 class="banner-title">زیورآلات زیبا</h2>

              <p style="color:rgb(235, 231, 238)" class="banner-text">
                شروع قیمت ها از  <b>80 هزار تومن</b>
              </p> 

              <!-- <a class="banner-btn" href="https://www.instagram.com/omde_paytakht" target="_blank" rel="noopener noreferrer">خرید کنید</a> -->

            </div>

          </div>
    <%})%>
              
    
    
            </div>
    
          </div>
    
        </div>
    
        <diV class="category">
    
          <div class="container">
        <section class="team">
          <h4 class="heading"> جدیدترین کالاها </h4>
    
          <div class="swiper team-slider">
              <div class="swiper-wrapper">
                  <% newPro.forEach((item)=>{%>
                    <div class="swiper-slide slide">
                        <div class="image">
                            <a href="/detailPost/<%=item.id%>">
                            <img src="<%=item.image%>"
                            alt="">
                        </a>
                            <div class="share">
                              <span>شیک ومجلسی</span>
                                <a href="/detailPost/<%=item.id%>">مشاهده کالا</a>
                               
                            </div>
                        </div>
                       
                    </div>
      
                 <% })%>
                
              </div>
              <div class="swiper-button-next"></div>
              <div class="swiper-button-prev"></div>
          </div>
      </section>
      <br/>
      <section class="vehicles" id="vehicles">
    
        <h3 class="heading"> محبوب ترین <span>محصولات</span> </h3>
    
        <div class="swiper vehicles-slider">
    
            <div class="swiper-wrapper">
              <% vPro.forEach((item)=>{ %>
                <div class="swiper-slide box">
                    <a href="/detailPost/<%=item.id%>">
                    <img src="<%=item.image%>" alt="">
                    </a>
                    <div class="content">
                        <h3>بهترین ها</h3>
                        
                      
                        <a href="/detailPost/<%=item.id%>">مشاهده</a>
                    </div>
                </div>
              <%})%>
              
            </div>
    
            <div class="swiper-pagination"></div>
    
        </div>
    
    </section>
    <section class="shop" id="shop">
    
      <div class="heading">
          <h3>پرفروشترین محصولات</h3>
         
      </div>
    
      <div class="swiper products-slider">
    
          <div class="swiper-wrapper">
           <% bestSell.forEach((item)=>{ %>
            <div class="swiper-slide slide">
                <div class="image">
                    <a href="/detailPost/<%=item.id%>">
                    <img src="<%=item.image%>" alt="">
                    </a>
                    <div class="icons">
                      
                       <a href="/detailPost/<%=item.id%>" class="fas fa-eye"></a>
                    </div>
                </div>
                <div class="content">
                  
                    <p>زیبا وارزان</p>
                    <div class="price"></div>
                </div>
            </div>
          <% })%>
    
  
          </div>
    
          <div class="swiper-button-next"></div>
          <div class="swiper-button-prev"></div>
    
      </div>
    
    </section>
    </div>
    </diV>
    
    
        <!--
          - CATEGORY
        -->
    
    
    
    
    
    
        <!--
          - PRODUCT
        -->
    
       
        <!--
          - TESTIMONIALS, CTA & SERVICE
        -->
    
       
        <!--
          - BLOG
        -->
    
       
    
      </main>
     
    <%- include('./partials/footer'); %> 
    <script src="https://unpkg.com/swiper@7/swiper-bundle.min.js"></script>
  
  <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
  <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <script src="/js/script.js"></script>
</body>
</html>